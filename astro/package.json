{
  "name": "skyshare",
  "type": "module",
  "version": "1.4.11",
  "scripts": {
    "dev": "PUBLIC_VERSION=$npm_package_version astro dev",
    "prod": "PUBLIC_IS_NOT_PRODUCTION=false PUBLIC_VERSION=$npm_package_version astro dev",
    "build": "astro check && PUBLIC_VERSION=$npm_package_version astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "PUBLIC_VERSION=$npm_package_version jest",
    "fix:format": "prettier --write",
    "fix:lint": "eslint --fix",
    "fix": "npm-run-all -p \"fix:* {1}\" --"
  },
  "dependencies": {
    "@astrojs/check": "^0.4.1",
    "@astrojs/cloudflare": "^8.1.0",
    "@astrojs/partytown": "^2.0.4",
    "@astrojs/react": "^3.0.9",
    "@astrojs/tailwind": "^5.1.0",
    "@atproto/api": "^0.9.2",
    "@types/js-base64": "^3.3.1",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "astro": "^4.1.2",
    "browser-image-compression": "^2.0.2",
    "cheerio": "^1.0.0-rc.12",
    "js-base64": "^3.7.6",
    "markdown-it": "^14.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.5.0",
    "tailwind-variants": "^0.1.20",
    "tailwindcss": "^3.4.1",
    "twitter-text": "^3.1.0",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/jest": "^29.5.12",
    "@types/markdown-it": "^13.0.7",
    "@types/twitter-text": "^3.1.10",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-react": "^7.34.1",
    "jest": "^29.7.0",
    "prettier-plugin-astro": "^0.13.0",
    "ts-jest": "^29.1.2"
  }
}
