---
interface Props {
    pageTitle: string
    cardPath: string
    robots: string[]
}
const { cardPath, pageTitle, robots } = Astro.props
import Header from "@/components/Header.astro"
import Footer from "@/components/Footer.astro"
import Blanklayout from "@/layouts/Blanklayout.astro"
---

<Blanklayout
    cardPath={cardPath}
    pageTitle={pageTitle}
    robots={robots}
    hidePostButton={false}
>
    <Header hideLoader={false} slot="header" />
    <slot />
    <Footer slot="footer" />
</Blanklayout>
